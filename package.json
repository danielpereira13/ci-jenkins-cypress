{
  "name": "cypress_example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "e2e": "cypress run",
    "e2e:smoke": "cypress run --spec \"cypress/integration/examples/navigation.spec.js\"",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:smoke:chrome": "cypress run --browser chrome --spec \"cypress/integration/examples/navigation.spec.js\"",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:smoke:firefox": "cypress run --browser firefox --spec \"cypress/integration/examples/navigation.spec.js\"",
    "e2e:record": "cypress run --record",
    "e2e:record:chrome": "cypress run --record --browser chrome",
    "e2e:record:edge": "cypress run --record --browser edge",
    "e2e:record:parallel": "cypress run --record --parallel",
    "test:ci": "run-p --race start:ci e2e",
    "test:ci:chrome": "run-p --race start:ci e2e:chrome",
    "test:ci:record": "run-p --race start:ci e2e:record",
    "test:ci:record:windows": "run-p --race start:ci:windows e2e:record",
    "test:ci:record:windows:chrome": "run-p --race start:ci:windows e2e:record:chrome",
    "test:ci:record:windows:edge": "run-p --race start:ci:windows e2e:record:edge",
    "test:ci:record:parallel": "run-p --race start:ci e2e:record:parallel",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "cy:open": "cypress open",
    "report:merge": "mochawesome-merge --reportDir cypress/results/json > cypress/results/mochawesome-bundle.json",
    "report:generate": "marge cypress/results/mochawesome-bundle.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/results/html/screenshots",
    "pretest": "npm run delete:reports",
    "testx": "cypress run",
    "delete:reports": "rm cypress/results/json/* || true",
    "merge:reports": "mochawesome-merge cypress/results/json/*.json > cypress/results/json/mochawesome-all.json",
    "generate:report": "marge -o cypress/report/final cypress/results/json/mochawesome-all.json",
    "test:junit": "cypress run --reporter junit --reporter-options mochaFile=results/Cypress/junit-[hash].xml",
    "test:mochawesome": "npm run testx && npm run merge:reports && npm run generate:report"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^4.8.0",
    "mocha": "8.0.1",
    "mochawesome": "6.1.1",
    "mochawesome-merge": "4.1.0",
    "mochawesome-report-generator": "5.1.0"

  }
}
