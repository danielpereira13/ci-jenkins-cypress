{
  "name": "cypress_example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "testxxxx": "echo \"Error: no test specified\" && exit 1",
    "e2e:all:electron": "npx cypress run",
    "e2e:all:chrome": "npx cypress run --browser chrome",
    "e2e:all:firefox": "npx cypress run --browser firefox --headless",
    "e2e:smoke:electron": "npx cypress run --spec \"cypress/integration/examples/actions.spec.js\"",
    "e2e:smoke:chrome": "npx cypress run --browser chrome --spec \"cypress/integration/examples/actions.spec.js\"",
    "e2e:smoke:firefox": "npx cypress run --browser firefox --headless --spec \"cypress/integration/examples/actions.spec.js\"",
    "e2e": "npx cypress run",
    "e2e:smoke": "npx cypress run --spec \"cypress/integration/examples/actions.spec.js\"",
    "e2e:smoketwo": "npx cypress run --spec \"cypress/integration/examples/actionstwo.spec.js\"",
    "e2e:chrome": "npx cypress run --browser chrome",
    "e2e:firefox": "npx cypress run --browser firefox",
    "e2e:record": "npx cypress run --record",
    "e2e:record:chrome": "npx cypress run --record --browser chrome",
    "e2e:record:edge": "npx cypress run --record --browser edge",
    "e2e:record:parallel": "npx cypress run --record --parallel",
    "test:ci": "run-p --race start:ci e2e",
    "test:ci:chrome": "run-p --race start:ci e2e:chrome",
    "test:ci:record": "run-p --race start:ci e2e:record",
    "test:ci:record:windows": "run-p --race start:ci:windows e2e:record",
    "test:ci:record:windows:chrome": "run-p --race start:ci:windows e2e:record:chrome",
    "test:ci:record:windows:edge": "run-p --race start:ci:windows e2e:record:edge",
    "test:ci:record:parallel": "run-p --race start:ci e2e:record:parallel",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "cy:run": "npx cypress run",
    "cy:run:record": "npx cypress run --record",
    "cy:open": "cypress open",
    "clean-reports": "rm -rf cypress/reports",
    "test": "npx cypress run",
    "merge-report": "npx mochawesome-merge --reportDir cypress/reports/separate-reports > cypress/reports/full_report.json",
    "generate-report": "npx mochawesome-report-generator --reportDir cypress/reports/html cypress/reports/full_report.json",
    "after:tests": "npm run merge-report; npm run generate-report",
    "cypressold": "npm run clean-reports; npm run test; npm run after:tests",
    "cypressx": "npm run clean-reports; npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^4.8.0",
    "mocha": "5.2.0",
    "mochawesome": "4.1.0",
    "mochawesome-merge": "2.0.1",
    "mochawesome-report-generator": "4.0.1"
  }
}
